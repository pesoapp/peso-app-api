model gives_address {
  address_id   Int     @id @default(autoincrement())
  customer_id  Int
  firstname    String  @db.VarChar(32)
  lastname     String  @db.VarChar(32)
  company      String  @db.VarChar(40)
  address_1    String? @db.VarChar(128)
  address_2    String? @db.VarChar(128)
  city         String  @db.VarChar(128)
  postcode     String  @db.VarChar(256)
  country_id   Int     @default(0)
  zone_id      Int     @default(0)
  custom_field String  @db.Text
  seller_id    Int?
  district     String? @db.VarChar(20)
  tracking_id  Int?
  region       String? @db.VarChar(128)

  @@index([address_1], map: "oca_address_1_index")
  @@index([address_2], map: "oca_address_2_index")
  @@index([address_id], map: "oca_address_id_index")
  @@index([city], map: "oca_city_index")
  @@index([company], map: "oca_company_index")
  @@index([country_id], map: "oca_country_id_index")
  @@index([customer_id], map: "oca_customer_id_index")
  @@index([firstname], map: "oca_firstname_index")
  @@index([lastname], map: "oca_lastname_index")
  @@index([postcode], map: "oca_postcode_index")
  @@index([seller_id], map: "oca_seller_id_index")
  @@index([zone_id], map: "oca_zone_id_index")
  @@map("4gives_address")
}

